#!/usr/bin/env python
try:
   import dotcloud_pkg
   from dotcloud_pkg.ui import CLI
except ImportError, e:
   import dotcloud
   from dotcloud.ui import CLI

if __name__ == '__main__':
   import os
   url = os.environ.get('DOTCLOUD_API_ENDPOINT', 'https://ws.dotcloud.com/1.0')
   cli = CLI(endpoint=url)
   cli.run()
